<% include ../partials/header %>


<div class="row">
    <div class="col-lg-12">
        <h3>Our Users</h3>
    </div>
    <% users.forEach(function(user) { %>
        <h4><a href="/users/<%= user._id %>" class="btn btn-primary"><%= user.firstName %> <%= user.lastName %></a></h4>
    <% }); %>
</div>



